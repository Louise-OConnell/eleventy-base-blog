---
layout: layouts/base.njk
templateClass: row
eleventyNavigation:
  key: blog
  order: latest
title: blog list  
---
<h1>{{ title }}</h1>

<p>
  <span style="font-weight: bold;">Rating:</span>
  {{ rating }}
</p>

{% for blogpost in collections.blog%}
   <a href="{{blogpost.url}}">{{blogpost.url}}</a>
{% endfor %}

<img src="{{ thumbnail | url }}"/>

{{ content | safe }}
